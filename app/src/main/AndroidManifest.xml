<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tomaszek.foodclicker">

    <application
        android:allowBackup="true"
        android:icon="@drawable/zboza"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PiramidActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Settings" />
        <activity android:name=".TabelaActivity" />
        <activity android:name=".WodaDetailsActivity" />
        <activity android:name=".InneDetailsActivity" />
        <activity android:name=".WarzywaDetailsActivity" />
        <activity android:name=".OwoceDetailsActivity"></activity>
    </application>

</manifest>