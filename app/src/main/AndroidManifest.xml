            android:name="com.doktornutria.tomaszek.foodclicker.OlejorzechDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.doktornutria.tomaszek.foodclicker.LogActivity"
            android:screenOrientation="portrait" />

        <service
            android:name="com.doktornutria.tomaszek.foodclicker.LogDBService"
            android:exported="false" />

        <activity android:name="com.doktornutria.tomaszek.foodclicker.AddProfileActivity" />
